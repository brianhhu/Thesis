@InProceedings{Roelfsema_etal08a,
  author    = {Roelfsema, P. R. and Poort, J. and Thiele, A. and Pooresmaeli, A.},
  title     = {{S}imultaneous decoding of attention and contrast from neuronal activity in the primary visual cortex on a single trial},
  booktitle = {SfN Abstract},
  year      = {2008},
  number    = {317.1},
  address   = {Washington, D.C.},
  publisher = {Society for Neuroscience},
  abstract  = {An optimal interaction with our environment requires the accurate representation of the sensory features of visual stimuli as well as their behavioural relevance. We showed previously that relevant, attended objects evoke more activity than unattended objects in the primary visual cortex (Roelfsema et al., 1998; Roberts et al., 2007). However, the activity of neurons in the primary visual cortex is also influenced by low-level features like variations in stimulus contrast. These low-level stimulus effects could degrade the reliability of the attentional code. Here we therefore investigated whether it is possible to decode the locus of attention as well as the contrast of a stimulus from a small population of neurons in area V1, on a single trial. We trained two monkeys on a task where they have to mentally trace a target curve while ignoring another, distractor curve, and varied the contrast of the curves. We obtained simultaneous recordings from 17 (monkey 1) or 39 multi-unit recording sites (monkey 2) using electrodes that were implanted chronically in area V1. To decode attention separately from contrast we used a support vector machine approach. Attention was decoded with 72\% and 81\% accuracy on single trials in the two monkeys in spite of the large variation of stimulus contrast, while relatively small differences in stimulus contrast (11\% and 19\% contrast) were also decoded with a high accuracy (>75\%). These results demonstrate that distinct and accurate representations of stimulus contrast and attention exist in area V1, and argue against ?contrast gain? theories of visual attention, which suggested that attention acts by increasing the apparent contrast of a stimulus in a neuron's receptive field. Such a contrast gain model confounds the effects of attention with those of contrast, and has difficulty explaining how subjects can direct their attention to low contrast image regions if these are the most relevant for the task at hand. The finding of separate codes for attention and contrast solves this problem and supports previous findings that attention does not always increase the apparent contrast of a stimulus. },
}

