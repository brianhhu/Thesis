@Article{Marom10,
  Title                    = {{Neural timescales or lack thereof}},
  Author                   = {Marom, Shimon},
  Journal                  = {Progress in Neurobiology},
  Year                     = {2010},

  Month                    = jan,
  Number                   = {1},
  Pages                    = {16--28},
  Volume                   = {90},

  Abstract                 = {This article aims at making readers, experimentalists and theorists, more aware of the abstractions made by an observer when measuring and reporting behavioral and neural timescales. These abstractions stow away the fact that, above lower boundaries that reflect fairly well understood physical constraints, observed and reported timescales are often not intrinsic to the biological system; rather, in most cases they reflect conditions that are imposed by the observer through the measuring procedure. This is true at practically every level of organization, from behavior down to molecules. I analyze the impacts of the resulting temporal manifold in behavioral and brain sciences on experimental designs and mathematical modeling.},
  Doi                      = {10.1016/j.pneurobio.2009.10.003},
  File                     = {:Users/asaf/Documents/Mendeley/Marom - 2010 - Neural timescales or lack thereof.pdf:pdf},
  ISSN                     = {1873-5118},
  Keywords                 = {Animal,Animal: physiology,Animals,Behavior,Behavior: physiology,Brain,Brain: metabolism,Humans,Kinetics,Models,Neurons,Neurons: metabolism,Research Design,Theoretical,Time Factors},
  Pmid                     = {19836433},
  Url                      = {http://www.ncbi.nlm.nih.gov/pubmed/19836433}
}

@Article{Marom09,
  Title                    = {{Adaptive transition rates in excitable membranes.}},
  Author                   = {Marom, Shimon},
  Journal                  = {Frontiers in Computational Neuroscience},
  Year                     = {2009},
  Pages                    = {2},
  Volume                   = {3},

  Abstract                 = {Adaptation of activity in excitable membranes occurs over a wide range of timescales. Standard computational approaches handle this wide temporal range in terms of multiple states and related reaction rates emanating from the complexity of ionic channels. The study described here takes a different (perhaps complementary) approach, by interpreting ion channel kinetics in terms of population dynamics. I show that adaptation in excitable membranes is reducible to a simple Logistic-like equation in which the essential non-linearity is replaced by a feedback loop between the history of activation and an adaptive transition rate that is sensitive to a single dimension of the space of inactive states. This physiologically measurable dimension contributes to the stability of the system and serves as a powerful modulator of input-output relations that depends on the patterns of prior activity; an intrinsic scale free mechanism for cellular adaptation that emerges from the microscopic biophysical properties of ion channels of excitable membranes.},
  Address                  = {Department of Physiology in the Faculty of Medicine and the Network Biology Research Laboratories, Technion - Israel Institute of Technology Haifa, Israel.},
  Doi                      = {10.3389/neuro.10.002.2009},
  File                     = {:Users/asaf/Documents/Mendeley/Marom - 2009 - Adaptive transition rates in excitable membranes.pdf:pdf},
  ISSN                     = {1662-5188 (Electronic)},
  Pmid                     = {19225576}
}

