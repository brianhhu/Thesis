@Article{Liebovitch_etal87,
  author   = {Liebovitch, Larry S. and Fischbarg, Jorge and Koniarek, Jan P. and Todorova, Irina and Wang, Mei},
  title    = {{Fractal model of ion-channel kinetics}},
  journal  = {Biochimica et Biophysica Acta (BBA) - Biomembranes},
