@InCollection{Indiveri_etal01,
  Title                    = {{A} competitive network of spiking {VLSI} neurons},
  Author                   = {Giacomo Indiveri and Timothy Horiuchi and Ernst Niebur and Rodney Douglas},
  Booktitle                = {World Congress on Neuroinformatics},
  Year                     = {2001},

  Address                  = {Vienna},
  Editor                   = {F. Rattay},
  Month                    = {September},

  Abstract_part1           = { giacomo@ini.phys.ethz.ch Networks of integrate and fire (I&F) neurons have been extensively studied using both analytical and numerical simulation tools[1]. An increasing number of researchers uses software implementations of these networks as tools for investigating the effect of spike timing and synchrony on the network's computational properties[2,3,4]. But while software simulations are effective for analizing networks of neurons that use rate codes, as the principal representation, detailed simulations of networks of spiking neurons is a CPU intensive process and can still require significant amount of simulation time. We present a set of hybrid analog/digital VLSI neural circuits, that allow us to overcome these problems, by exploiting the advantages of both highly parallel analog computation and high-speed asynchronous digital VLSI techniques. Specifically, we present a VLSI device that contains a competitive network of I&F neurons, implemented using analog circuits, and a communication framework for receiving and sending spikes off-chip, implemented using hybrid analog/digital circuits. The network's architecture consists of a ring of 32 neurons that excite a global inhibitory neuron. The inhibitory neuron in turn projects its output spikes back to all the excitatory neurons via inhibitory synapses. The excitatory neurons are recurrently connected to their nearest neighbours with excitatory synapses. Each neuron has can also be excited or inhibited via additional synaptic circuits that can be accessed by spikes arriving from outside the chip. Architectures of this type have been shown to exhibit a broad range of interesting computational properties[5,6]. },
  Abstract_part2           = {In [5]the authors present a similar network architecture, but in which the neurons are modeled with simple linear threshold units. In the mean-rate interpretation of neural firing the network we propose can exhibit a winner-take-all behavior and reproduce other results presented in [5]. In the more realistic spiking interpretation of the network, the system can also produce behaviors such as adaptive thresholding, single-neuron winner-take-all behavior, and synchrony filtering. The temporal dependence of inhibition produces a variable selectivity to synchronous events as well as a locking behavior that shifts the output spikes more closely to the input ones in time. In addition to being a useful tool for investigating in real-time the computational properties of spiking networks, the chip presented allows us to test these spike-based computational models in real physical sensorimotor systems, by connecting the chip to other neuromorphic sensor chips (e.g. silicon retinas and cochleae) and to robotic actuators. [1] W. Maass and C. M. Bishop, Pulsed Neural Networks, MIT Press, 1998. [2] O. Bernander, C. Koch, and M. Usher, The effect of synchronized inputs at the single neuron level, Neural Computation, 6:622-641, 1994. [3] P. Koenig, A. K. Engel, P. R. Roelfsema, and Wolf Singer, How precise is neuronal synchronization? Neural Computation, 7:469-485, 1995. [4] M. Diesmann, M-O. Gewaltig, and A. Aertsen, Stable propagation of synchronous spiking in cortical neural networks, Nature, 402:529-533, 1999. [5] R. Hahnloser, R. Sarpeshkar, M. Mahowald, R. J. Douglas, and S. Seung, Digital selection and analog amplification co-exist in an electronic circuit inspired by neocortex, Nature, 405:947-951, 2000. [6] E.. Salinas and L.F. Abbott, A model of multiplicative neural responses in parietal cortex, Proc. Natl. Acad. Sci., 93:11956-11961, 1996.},
  Our_pages                = {1-12},
  Publink                  = {Indiveri_etal01.pdf}
}

